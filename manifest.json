{
    "name": "covid extension",
    "description": "Covid information related to a chosen country",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./world_icon.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "browser_action": {
        "default_popup": "./popup.html"
    },
    "permissions": ["<all_urls>"]
}